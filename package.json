{
  "name": "webdriverio_v5_typescript",
  "version": "1.0.0",
  "description": "Boilerplate project using version 5 WebdriverIO & TypeScript.",
  "repository": "https://github.com/jpolley/WebdriverIO_v5_TypeScript",
  "scripts": {
    "test": "wdio",
    "lint": "tslint --fix 'test/**/*.ts' && tslint --fix 'src/**/*.ts'",
    "allure:history": "mkdir -p allure-report/history ./reports/allure-results/history && cp -a allure-report/history ./reports/allure-results/",
    "report": "allure generate ./reports/allure-results --clean && allure open",
    "report-ci": "allure generate ./reports/allure-results --clean -o allure-report"
  },
  "author": "Jeremy Polley",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/node": "^14.14.22",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/cli": "^6.12.1",
    "@wdio/cucumber-framework": "^6.11.1",
    "@wdio/local-runner": "^6.12.1",
    "@wdio/mocha-framework": "^6.11.0",
    "@wdio/selenium-standalone-service": "^6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/sync": "^6.11.0",
    "allure-commandline": "^2.13.8",
    "chai": "^4.2.0",
    "chromedriver": "^88.0.0",
    "cucumber-tag-expressions": "^2.0.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.12.1"
  }
}
